{
  "extends": [
    "magic/typescript"
  ],
  "globals": {
  },
  "rules": {
    "no-bitwise": 0,
    "prefer-arrow-callback": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "ts": "never"
      }
    ],
    "import/no-unresolved": "off",
    "no-useless-concat": "off"
  }
}
